fn main() {
    let my_number = 12345;
    let num_as_string = i32_to_string(my_number);
    let message = string_concat("The number is: ", num_as_string);
    print(message);

    // 負の数と0のテスト
    print(string_concat("A negative number: ", i32_to_string(-987)));
    print(string_concat("Zero: ", i32_to_string(0)));
}
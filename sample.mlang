// プログラムのエントリーポイント
fn main() {
    let s1 = "Hello";
    let s2 = ", ";
    let s3 = "World!";
    
    // 文字列を動的に連結します
    // 1. tmp = string_concat "Hello" ", " -> "Hello, "
    // 2. greeting = string_concat "Hello, " "World!" -> "Hello, World!"
    let tmp = string_concat s1 s2;
    let greeting = string_concat tmp s3;
    
    // 動的に生成された文字列を出力します
    print greeting;
}